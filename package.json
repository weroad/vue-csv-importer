{
  "name": "@weroad/csv-importer",
  "version": "1.0.0",
  "description": "A Vue3 CSV importer component with validation and editing capabilities",
  "main": "dist/index.js",
  "module": "dist/index.mjs",
  "types": "dist/index.d.ts",
  "exports": {
    ".": {
      "types": "./dist/index.d.ts",
      "import": "./dist/index.mjs",
      "require": "./dist/index.js"
    },
    "./style.css": "./dist/style.css"
  },
  "files": [
    "dist"
  ],
  "scripts": {
    "build": "vite build",
    "dev": "vite build --watch",
    "test": "vitest",
    "test:ci": "vitest run",
    "type-check": "vue-tsc --noEmit",
    "format": "prettier --write .",
    "lint": "eslint .",
    "lint:fix": "eslint . --fix",
    "lint:ci": "eslint . --cache --cache-strategy=content"
  },
  "dependencies": {
    "vue": "^3.0.0",
    "@tanstack/table-core": "^8.21.3",
    "@tanstack/vue-table": "^8.21.3",
    "papaparse": "^5.4.1",
    "tippy.js": "^6.3.7",
    "vue-tippy": "^6.4.1",
    "zod": "^4.0.17"
  },
  "devDependencies": {
    "@eslint/js": "^9.33.0",
    "@microsoft/api-extractor": "^7.52.11",
    "@types/papaparse": "^5.3.14",
    "@typescript-eslint/eslint-plugin": "^8.40.0",
    "@typescript-eslint/parser": "^8.40.0",
    "@vitejs/plugin-vue": "^4.5.0",
    "autoprefixer": "^10.4.21",
    "eslint": "^9.33.0",
    "eslint-plugin-vue": "^10.4.0",
    "jsdom": "^26.1.0",
    "postcss": "^8.5.6",
    "prettier": "3.6.2",
    "tailwindcss": "^3.4",
    "typescript": "^5.0.0",
    "typescript-eslint": "^8.40.0",
    "unplugin-dts": "1.0.0-beta.0",
    "vite": "^5.0.0",
    "vitest": "^3.2.4",
    "vue-eslint-parser": "^10.2.0",
    "vue-tsc": "^1.8.0"
  },
  "keywords": [
    "vue3",
    "csv",
    "importer",
    "validation",
    "zod"
  ],
  "author": "Danilo Polani",
  "license": "MIT"
}
